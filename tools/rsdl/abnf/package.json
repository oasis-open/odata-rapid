{
  "name": "rsdl-abnf",
  "version": "0.0.0",
  "description": "RSDL ABNF Grammar",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "extract-abnf": "node lib/extract.js",
    "generate-grammar": "node node_modules/apg-js/bin/apg.sh --strict -i ./rsdl.abnf -o lib/grammar.js",
